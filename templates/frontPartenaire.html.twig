        <div class="block about">
          <!-- footer logo -->
          <img src={{ asset('images/logoo.png') }} alt="logo">
          <!-- description -->
           <p class="alt-color">Collecti est une plateforme d'enchères en ligne spécialisée dans les objets de collection.
           </p>
        </div>
      </div>
      <!-- Link list -->
