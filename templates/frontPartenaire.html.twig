              <img src="{{asset('front/images/footer/phone-icon.png')}}" alt="mobile-icon">
            </a>
            <p class="mb-0">l'application mobile sera lancée prochainement</p>
          </div>
          <div class="download-btn d-flex my-3">
            <a href="index.html"><img src="{{asset('images/apps/google-play-store.png')}}" class="img-fluid" alt=""></a>
            <a href="index.html" class=" ml-3"><img src="{{asset('front/images/apps/apple-app-store.png')}}" class="img-fluid" alt=""></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Container End -->
